          body:
            application/json:
              schema: "{\n  \"$schema\": \"http://json-schema.org/draft-04/schema\"\
                ,\n  \"definitions\": {\n    \"timeout\": {\n      \"type\" : \"integer\"\
                ,\n      \"minimum\": -1\n    }\n  }\n}\n"
              example: "{\n  \"timeout\" : 20\n}\n"
    put:
      description: |
        Set a new browser session timeout, expressed in minutes.
        Specify `-1` for no timeout.
      protocols: [HTTPS]
      body:
        application/json:
          schema: "{\n  \"$schema\": \"http://json-schema.org/draft-04/schema\",\n\
            \  \"definitions\": {\n    \"timeout\": {\n      \"type\" : \"integer\"\
            ,\n      \"minimum\": -1\n    }\n  }\n}\n"
          example: "{\n  \"timeout\" : 20\n}\n"
      responses:
