    - authenticated
    - supported
    uriParameters:
      +path:
        description: |
          Specifies constraints on the events to retrieve.  See the
          [specifying constraints section of /events](#events) for details.
        type: string
        required: true
    get:
      description: Queries Log Insight for groups of events.
      protocols: [HTTPS]
      is:
