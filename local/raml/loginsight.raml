                \ \"oneOf\": [\n            {\"$ref\":\"#/definitions/UpgradeStatusSingleNode\"\
                }\n          ]}\n        },\n        \"pakFileUrl\": {\n         \
                \ \"type\": \"string\"\n        },\n        \"eulaAccepted\": {\n\
                \          \"type\": \"boolean\"\n        },\n        \"masterToken\"\
                : {\n          \"type\": \"string\"\n        },\n        \"clusterStatus\"\
                : {\n          \"enum\": [\n            \"Started\",\n           \
